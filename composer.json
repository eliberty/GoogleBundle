{
    "name": "eliberty/googlebundle",
    "type": "symfony-bundle",
    "description": "Google Bundle for Symfony2",
    "keywords": ["eliberty", "adwords", "analytics", "ecommerce", "enhanced ecommerce", "google", "impression", "product", "maps", "symfony"],
    "homepage": "http://github.com/eliberty/googlebundle",
    "license": "MIT",
    "authors": [
        {"name": "Matthew Fitzgerald", "email": "matthewfitz@gmail.com"}
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://e-liberty.repo.repman.rpstack.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "eliberty/antimattr-common-product": "~1.0@stable"
    },
    "autoload": {
        "psr-4": {
            "AntiMattr\\GoogleBundle": ""
        }
    },
    "archive": {
        "exclude": ["bin", "Tests", "*phpunit.xml"]
    },
    "target-dir" : "eliberty/GoogleBundle"
}
